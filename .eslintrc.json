{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "class-methods-use-this": 0,
        "no-console": 0,
        "max-len": 0,
        "no-promise-executor-return": 0
    }
}
